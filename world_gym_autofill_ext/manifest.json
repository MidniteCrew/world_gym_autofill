{
  "manifest_version": 2,
  "name": "World Gym Guest Autofill",
  "version": "1.1",
  "description": "Autofills the World Gym guest registration form using locally stored data.",

  "permissions": [
    "storage",
    "https://www.ggpx.info/*"
  ],

  "browser_action": {
    "default_popup": "popup.html"
  },

  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },

  "content_scripts": [
    {
      "matches": ["https://www.ggpx.info/guestreg*"],
      "js": ["content.js"]
    }
  ],

  "browser_specific_settings": {
    "gecko": {
      "id": "world-gym-autofill@gabrielungur",
      "strict_min_version": "109.0",
      "data_collection_permissions": {
        "required": ["none"],
        "collects_personal_data": false,
        "collects_location_data": false,
        "collects_browsing_history": false,
        "collects_user_activity": false,
        "collects_health_data": false,
        "collects_financial_data": false,
        "collects_authentication_info": false,
        "collects_contacts": false,
        "collects_messages": false,
        "collects_audio": false,
        "collects_video": false,
        "collects_photos": false,
        "collects_files": false
      }
    }
  }
}